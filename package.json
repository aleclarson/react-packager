{
  "name": "react-packager",
  "version": "0.2.0",
  "engines": {
    "node": ">=4"
  },
  "peerDependencies": {
    "react-native": ">=0.22"
  },
  "lotus": {
    "dest": "js",
    "plugins": [
      "lotus-babel"
    ],
    "implicitDependencies": [
      "os",
      "fs",
      "NativeModules",
      "child_process",
      "crypto",
      "events",
      "AssetRegistry",
      "net",
      "path",
      "prepack",
      "url",
      "util"
    ]
  },
  "dependencies": {
    "Promise": "aleclarson/Promise#1.0.0",
    "assert": "aleclarson/assert#1.0.0",
    "babel-core": "aleclarson/babel-core#6.9.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-preset-fbjs": "^2.0.0",
    "babel-preset-react-native": "aleclarson/babel-preset-react-native#1.5.2",
    "babel-types": "6.6.x",
    "bser": "^1.0.2",
    "debug": "^2.2.0",
    "emptyFunction": "aleclarson/emptyFunction#1.0.0",
    "graceful-fs": "^4.1.4",
    "image-size": "^0.3.5",
    "io": "aleclarson/io#2.1.0",
    "joi": "^6.10.1",
    "log": "aleclarson/log#1.0.0",
    "lotus-require": "aleclarson/lotus-require#3.1.0",
    "micromatch": "^2.3.8",
    "node-haste": "aleclarson/node-haste#2.12.0",
    "sync": "aleclarson/sync#1.0.0",
    "temp": "^0.8.3",
    "uglify-js": "^2.4.24",
    "underscore": "1.8.x",
    "worker-farm": "^1.3.1"
  },
  "devDependencies": {
    "jest-cli": "git://github.com/facebook/jest#0.5.x"
  },
  "jest": {
    "setupEnvScriptFile": "jestSupport/env.js",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "source-map"
    ]
  }
}
